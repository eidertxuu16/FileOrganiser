{
  "manifest_version": 3,
  "name": "Download Organizer",
  "description": "Automatically organize your downloads into folders.",
  "version": "1.0.3",
  "permissions": [
    "downloads",
    "storage",
    "tabs",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}